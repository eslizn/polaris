{
  "name": "eslizn/polaris",
  "homepage": "https://github.com/eslizn/polaris",
  "description": "app framework for php",
  "type": "library",
  "license": "MIT",
  "authors": [
	{
	  "name": "eslizn",
	  "email": "eslizn@gmail.com"
	}
  ],
  "autoload": {
	"psr-4": {
	  "Polaris\\": "src/"
	},
	"files": [
	  "helpers.php"
	]
  },
  "autoload-dev": {
	"psr-4": {
	  "Tests\\": "tests/"
	},
	"files": [
	  "helpers.php"
	]
  },
  "minimum-stability": "stable",
  "require": {
	"php": ">=7.1",
	"psr/http-message": "^1.0",
	"psr/http-server-middleware": "^1.0",
	"nikic/fast-route": "^1.3",
	"psr/log": "^1.0",
	"ext-json": "*",
	"ext-pdo": "*",
	"ext-sockets": "*"
  },
  "require-dev": {
	"eaglewu/swoole-ide-helper": "dev-master",
	"phpunit/phpunit": "^9.5"
  }
}
