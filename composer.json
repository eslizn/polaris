{
  "name": "eslizn/polaris",
  "homepage": "https://github.com/eslizn/polaris",
  "description": "app framework for php",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "eslizn",
      "email": "eslizn@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Polaris\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Polaris\\Tests\\": "tests/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4",
    "psr/container": "^1",
    "psr/event-dispatcher": "^1",
    "psr/http-server-middleware": "^1",
    "psr/log": "^1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "vlucas/phpdotenv": "^5",
    "swoole/ide-helper": "dev-master"
  }
}